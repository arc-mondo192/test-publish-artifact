#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/acorn@8.9.0/node_modules/acorn/bin/node_modules:/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/acorn@8.9.0/node_modules/acorn/node_modules:/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/acorn@8.9.0/node_modules:/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/acorn@8.9.0/node_modules/acorn/bin/node_modules:/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/acorn@8.9.0/node_modules/acorn/node_modules:/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/acorn@8.9.0/node_modules:/mnt/6d89a436-a512-4b67-9b80-e7ac1b94d2b8/personal/test-publish-artifact/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
